<uwsgi>
	<socket>localhost:8307</socket>
	<listen>10</listen>
	<plugins>python</plugins>
	<master>true</master>
	<pidfile>/run/uwsgi/app/wepay_web/pid</pidfile>
	<processes>1</processes>
	<module>MerchantManage.wsgi</module>
	<pythonpath>/home/wepay/wepay_web/MerchantManage</pythonpath>
	<profiler>true</profiler>
	<memory-report>true</memory-report>
	<enable-threads>true</enable-threads>
	<logdate>true</logdate>
	<limit-as>6048</limit-as>
</uwsgi>
